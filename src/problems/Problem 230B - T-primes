int is_prime(long long  n)
{
	if (n == 2)
		return 1;
	if (n < 2 || n % 2 == 0)
		return 0;
	for (int i = 3; i * i <= n; i += 2)
		if (n % i == 0)
			return 0;
	return 1;
}
	int main()
	{
		int n;
		cin >> n;
		while (n--)
		{
			long long x;
			cin >> x;
			if (sqrt(x) == (int)sqrt(x) && is_prime(sqrt(x)))
				cout << "YES"<<endl ;
			else
				cout << "NO"<<endl ;
		}
		return 0;
	}
